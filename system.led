config led 'led_wifi'
	option name 'wifi'
	option sysfs 'bthomehubv5a:blue:wireless'
	option default '0'
	option trigger 'netdev'
	option dev 'wlan1'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'

config led 'led_dsl'
	option name 'dsl'
	option sysfs 'bthomehubv5a:blue:broadband'
	option default '0'
	option trigger 'netdev'
	option dev 'dsl0'
	list mode 'link'
	list mode 'tx'
	list mode 'rx'
